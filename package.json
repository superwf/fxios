  "version": "0.7.0",
  "description": "fetch generator customize",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "scripts": {
    "start": "jest --watch",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rollup -c && git add dist",
    "prettier": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@git.jd.com:wangfan15/fetch-maker.git"
  },
  "keywords": [
    "ajax",
    "fetch",
    "request"
  ],
  "author": "wangfan15@jd.com",
  "license": "ISC",
  "dependencies": {
    "@types/events": "^1.2.0",
    "babel-preset-react-app": "^3.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "path-to-regexp": "^2.2.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-preset-react-app": "^3.1.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
