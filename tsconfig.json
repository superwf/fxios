{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "noImplicitUseStrict": true,
    "lib": ["dom", "es2015", "es5", "es6"]
  }
}
