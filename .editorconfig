# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{ts,js,py,tsx,jsx,json}]
charset = utf-8
indent_size = 2
indent_style = space
